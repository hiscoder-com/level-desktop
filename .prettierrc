{
  "parser": "flow",
  "useTabs": false,
  "printWidth": 90,
  "tabWidth": 2,
  "trailingComma": "es5",
  "singleQuote": true,
  "bracketSameLine": false,
  "semi": false,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^react$",
    "",
    "^next/",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/app/(.*)$",
    "",
    "^[./]",
    "",
    "^public/(.*)$"
  ],
  "tailwindConfig": "./renderer/tailwind.config.js"
}
